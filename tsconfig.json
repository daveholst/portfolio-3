{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true
    },
    "paths": {
        "$types/*": [
            "src/types/*"
        ],
        "$lib/*": [
            "src/lib/*"
        ],
        "$components/*": [
            "./src/components/*"
        ],
        "$images/*": [
            "static/images/"
        ]
    }
}